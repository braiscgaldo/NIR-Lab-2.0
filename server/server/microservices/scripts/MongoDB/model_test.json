{
    "name": "model_1",
    "target": "Graduation",
    "num_epochs": 10,
    "loss_function": "binary_crossentropy",
    "learning_rate": 0.01,
    "epsilon": 0.05,
    "optimizer": "adam",
    "metrics": ["accuracy"],
    "batch_size": 32,
    "decay": 0.05,
    "train_size": 0.6,
    "validation_size": 0.25,
    "test_size": 0.15,
    "layers": {
        "1": {
            "name": "Dense",
            "units": 48,
            "activation": "relu"
        },
        "2": {
            "name": "Dense",
            "units": 16,
            "activation": "sigmoid"
        },
        "3": {
            "name": "Dense",
            "units": 1,
            "activation": "softmax"
        }
    }
}